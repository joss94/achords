<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="ChordButton">
        <attr name="textColor" format="color"/>
        <attr name="color" format="color" />
        <attr name="note" format="enum">
            <enum name="A" value="0"/>
            <enum name="B" value="1"/>
            <enum name="C" value="2"/>
            <enum name="D" value="3"/>
            <enum name="E" value="4"/>
            <enum name="F" value="5"/>
            <enum name="G" value="6"/>
        </attr>
        <attr name="mode" format="enum">
            <enum name="major" value="0"/>
            <enum name="minor" value="1"/>
            <enum name="seven" value="2"/>
            <enum name="seven_major" value="3"/>
            <enum name="four" value="4"/>
            <enum name="six" value="5"/>
        </attr>
        <attr name="attribute" format="enum">
            <enum name="none" value="0"/>
            <enum name="d" value="1"/>
            <enum name="b" value="2"/>
        </attr>
        <attr name="textSize" format="dimension"/>
    </declare-styleable>

</resources>